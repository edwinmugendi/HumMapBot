{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:app55/app55-php.git"
        }
    ],
    "require": {
        "laravel/framework": "4.1.*",
        "patricktalmadge/bootstrapper": "dev-develop",
        "nesbot/Carbon": "*",
        "facebook/php-sdk": "*",
        "guzzlehttp/guzzle": "~4.0",
        "app55/app55-php": "*",
        "arcturial/clickatell": "*",
        "urbanairship/urbanairship": "dev-master"
    },
    "autoload": {
        "classmap": [
            "app/controllers",
            "app/models",
            "app/database/seeds",
            "app/tests/TestCase.php",
            "app/libraries",
            "app/facades",
            "app/providers",
            "app/exceptions"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
